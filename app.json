{
  "expo": {
    "name": "Simple Weather",
    "slug": "weather-mobile",
    "version": "3.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*",
      "src/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5662395825140930~7013359044"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0062ff"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5662395825140930~7013359044"
      },
      "package": "weather.mobile",
      "permissions": ["LOCATION", "ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "versionCode": 20
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
